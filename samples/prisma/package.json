{
  "name": "@samples/prisma",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "migrate": "npx prisma migrate dev --name init"
  },
  "nx": {
    "targets": {
      "build": {
        "executor": "@nx/esbuild:esbuild",
        "outputs": [
          "{options.outputPath}"
        ],
        "defaultConfiguration": "production",
        "options": {
          "platform": "node",
          "outputPath": "samples/prisma/dist",
          "format": [
            "cjs"
          ],
          "bundle": false,
          "main": "samples/prisma/src/main.ts",
          "tsConfig": "samples/prisma/tsconfig.app.json",
          "assets": [
            "samples/prisma/src/assets"
          ],
          "esbuildOptions": {
            "sourcemap": true,
            "outExtension": {
              ".js": ".js"
            }
          }
        },
        "configurations": {
          "development": {},
          "production": {
            "esbuildOptions": {
              "sourcemap": false,
              "outExtension": {
                ".js": ".js"
              }
            }
          }
        }
      },
      "serve": {
        "executor": "@nx/js:node",
        "defaultConfiguration": "development",
        "dependsOn": [
          "build"
        ],
        "options": {
          "buildTarget": "@samples/prisma:build",
          "runBuildTargetDependencies": false
        },
        "configurations": {
          "development": {
            "buildTarget": "@samples/prisma:build:development"
          },
          "production": {
            "buildTarget": "@samples/prisma:build:production"
          }
        }
      }
    }
  },
  "dependencies": {
    "@prisma/client": "^6.5.0",
    "prisma": "^6.5.0"
  }
}
